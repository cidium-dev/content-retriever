{
  "name": "content-retriever",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir ./build --target bun",
    "start": "NODE_ENV=production bun src/index.ts",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix"
  },
  "dependencies": {
    "@anthropic-ai/tokenizer": "^0.0.4",
    "@bogeychan/elysia-logger": "^0.0.17",
    "@mozilla/readability": "^0.5.0",
    "axios": "^1.7.7",
    "elysia": "^0.8.17",
    "franc": "^6.2.0",
    "googleapis": "^134.0.0",
    "is-url": "^1.2.4",
    "js-tiktoken": "^1.0.14",
    "jsdom": "^24.1.3",
    "mammoth": "^1.8.0",
    "moment": "^2.30.1",
    "pdf-parse": "^1.1.1",
    "pino": "^8.21.0",
    "pino-pretty": "^10.3.1",
    "puppeteer": "^22.15.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-anonymize-ua": "^2.4.6",
    "puppeteer-extra-plugin-block-resources": "^2.4.3",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jsdom": "^21.1.7",
    "@types/pdf-parse": "^1.1.4",
    "gts": "^5.3.1",
    "typescript": "~5.2.2"
